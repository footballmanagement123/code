        const app = firebase.initializeApp(firebaseConfig);
    const database = firebase.database();

    const clubBotsRef = database.ref('clubs/bots/clubsBasic');

    clubBotsRef.on('child_added', (snapshot) => {
      const club = snapshot.val();

      if (club.country === 'Spain') {
        displayBlackCircleOnMap();
      }
    });

    function displayBlackCircleOnMap() {
      const mapContainer = document.getElementById('map-container');
      const path = document.querySelector('path');
      
      // Get the bounding box of the path
      const pathBounds = path.getBoundingClientRect();

      // Calculate the coordinates within the path
      const x = 100 + pathBounds.left; // Adjust the X coordinate as needed
      const y = 100 + pathBounds.top; // Adjust the Y coordinate as needed

      // Create a new <circle> element
      const circle = document.createElementNS('http://www.w3.org/2000/svg', 'circle');

      // Set attributes for the circle element
      circle.setAttribute('cx', x); // X coordinate of the center of the circle
      circle.setAttribute('cy', y); // Y coordinate of the center of the circle
      circle.setAttribute('r', '10'); // Radius of the circle
      circle.setAttribute('fill', 'black'); // Fill color of the circle

      // Append the circle element to the map container
      mapContainer.appendChild(circle);
    }
